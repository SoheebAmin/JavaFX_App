<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<GridPane alignment="center" hgap="10" prefHeight="339.0" prefWidth="375.0" vgap="10" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sampleJavaFX.entrySceneController">
   <columnConstraints>
      <ColumnConstraints />
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
   </rowConstraints>
   <children>
      <AnchorPane fx:id="theBox" prefHeight="600.0" prefWidth="800.0">
         <children>
            <VBox fx:id="Pick" layoutX="4.0" prefHeight="256.0" prefWidth="180.0" spacing="10.0">
               <children>
                  <Label prefHeight="26.0" prefWidth="130.0" text="Check Boxes">
                     <font>
                        <Font name="System Bold" size="20.0" />
                     </font>
                  </Label>
                  <CheckBox fx:id="firstbox" mnemonicParsing="false" text="First" />
                  <CheckBox fx:id="secondbox" mnemonicParsing="false" text="Second" />
                  <CheckBox fx:id="thirdbox" mnemonicParsing="false" text="Third" />
                  <Button fx:id="runBtn" mnemonicParsing="false" onAction="#runClicked" text="Run" />
                  <Label fx:id="theLabel" text="Label" />
               </children>
               <padding>
                  <Insets left="10.0" top="10.0" />
               </padding>
            </VBox>
            <Button layoutX="206.0" layoutY="147.0" mnemonicParsing="false" onAction="#changeToSecondScene" text="Second Scene" />
         </children>
      </AnchorPane>
   </children>
</GridPane>
